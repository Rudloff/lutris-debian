#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.6.0+git at
# Mon, 15 Sep 2014 23:00:13 +0200

%:
	dh $@ --with python2 --buildsystem=python_distutils

override_dh_auto_install:
	python setup.py install --root=debian/lutris --install-layout=deb --install-scripts=/usr/games
	gzip -9 -c -n debian/ChangeLog > debian/changelog.gz
	dh_installdocs debian/changelog.gz 

override_dh_auto_clean:
	dh_auto_clean
	rm -f debian/changelog.gz
